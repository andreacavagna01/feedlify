#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/feedlify/node_modules/.pnpm/typeorm@0.3.20_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.24_typescript@5.3.3_/node_modules/typeorm/node_modules:/workspaces/feedlify/node_modules/.pnpm/typeorm@0.3.20_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.24_typescript@5.3.3_/node_modules:/workspaces/feedlify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/feedlify/node_modules/.pnpm/typeorm@0.3.20_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.24_typescript@5.3.3_/node_modules/typeorm/node_modules:/workspaces/feedlify/node_modules/.pnpm/typeorm@0.3.20_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.24_typescript@5.3.3_/node_modules:/workspaces/feedlify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typeorm@0.3.20_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.24_typescript@5.3.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typeorm@0.3.20_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.24_typescript@5.3.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
